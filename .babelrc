{
  "presets": [
    [
      "es2015",
      {
        "modules": false
      }
    ],
    "react",
    "stage-0",
    "stage-1",
    "stage-2",
    "stage-3"
  ],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    "react-hot-loader/babel",
    "transform-async-to-generator",
    "transform-regenerator",
    [
      "import",
      {
        "libraryName": "antd"
      }
    ]
  ]
}